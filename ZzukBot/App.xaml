<Application x:ClassModifier="internal" x:Class="ZzukBot.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:ZzukBot" >
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="GUI/Utilities/Dictionaries/Commands.xaml" />
                <ResourceDictionary Source="GUI/Utilities/Dictionaries/Converters.xaml" />
                <ResourceDictionary Source="GUI/Utilities/Dictionaries/LayoutResources.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
